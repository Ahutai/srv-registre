if listfolderiii 
  each post in listfolderiii
    div(class="divlist3s")
      //- if post.updown === "UP"
      //- div(class="up-down active")
      img(src="/images/folderrrr.png", alt="" class="folder")
      //- else 
      //-   div(class="up-down")
      if editable === false
        div(class="divlist4")
          a(class="divlist5" href="/users/sposts/" + post._id)= post.name     
      else 
        div(class="divlist4")
          a(class="divlist5" href="/posts/findeddd/" + post._id)= post.name
      div(class="divlist4")
        a(class="divlist54") 1 Mo
      div(class="divlist4")
        a(class="divlist555")= post.date
      div(class="divlist4")
        p(class="divlist5555") FOLDER
      if editable === false
        div(class="divbutton")
          a(href="#") 
            a(src="", alt="" class="ajout")
          a(src="", alt="" class="del" )
          a(src="", alt="" class="trs")
      else
        div(class="divbutton")
          a( href="/posts/spost/" + post._id) 
            img(src="/images/add2d.svg", alt="" class="addd")
          a(href="/posts/modfoldernivii/" + post._id) 
            img(src="/images/troisd.svg", alt="" class="buttonmod")
          img(src="/images/deleted.svg", alt="" class="buttonmlistt" postid=post._id postidd=post.author)
